{
	"compilerOptions": {
		"target": "ESNext",
		"lib": ["DOM", "ESNext"],
		"module": "ESNext",
		"moduleResolution": "node",
		"jsx": "react-jsx",
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"allowSyntheticDefaultImports": true,
		"resolveJsonModule": true
	},
	"include": ["src", "global.d.ts"],
	"exclude": ["node_modules", "dist"]
}
